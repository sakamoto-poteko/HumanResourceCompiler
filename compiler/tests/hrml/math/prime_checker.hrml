function is_prime(n) {
    if (n <= 1) {
        return false; // Not prime
    }
    let i;
    for (i = 2, i * i <= n, ++i) {
        if (n % i == 0) {
            return false; // Not prime
        }
    }
    return true; // Prime
}

sub start() {
    while (true) {
        let number = inbox();
        // let prime = ;
        outbox(is_prime(number));
    }
}


