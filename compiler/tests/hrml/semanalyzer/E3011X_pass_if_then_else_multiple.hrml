function test()
{
    if (inbox()) {
        return 1;
    } 
    
    if (inbox()) {
        return 0;
    }

    if (inbox()) {
        return 0;
    }

    return 2; // useless return
}

function test2()
{
    if (inbox()) {
        return 0;
    } else {
        return 1;
    }
    // no return outside if
}

sub start()
{
    test();
    test2();
}
