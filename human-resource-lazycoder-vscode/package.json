{
  "name": "human-resource-lazycoder",
  "displayName": "Human Resource Machine LazyCoder",
  "description": "Language support for Human Resource Machine LazyCoder",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hrml",
        "aliases": [
          "Human Resource Machine LazyCoder",
          "hrml"
        ],
        "extensions": [
          "hrml"
        ],
        "configuration": "./hrml-language-configuration.json"
      },
      {
        "id": "hrasm",
        "aliases": [
          "Human Resource Machine LazyCoder IR",
          "hrasm"
        ],
        "extensions": [
          "hrasm"
        ],
        "configuration": "./hrasm-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hrml",
        "scopeName": "source.hrml",
        "path": "./syntaxes/hrml.tmLanguage.json"
      },
      {
        "language": "hrasm",
        "scopeName": "source.hrasm",
        "path": "./syntaxes/hrasm.tmLanguage.json"
      }
    ]
  },
  "publisher": "sakamoto-poteko",
  "repository": {
    "url": "https://github.com/sakamoto-poteko/HumanResourceCompiler"
  }
}
