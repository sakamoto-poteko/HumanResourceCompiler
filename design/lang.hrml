let a = 0;
let b = 0;

function bubble_sort() {
    let swapped = 1; // Initialize swapped to enter the loop

    while (swapped == 1) {
        swapped = 0;

        // Compare and swap a and b
        if (a > b) {
            let temp = a;
            a = b;
            b = temp;
            swapped = 1;
        }
    }

    let x = a;
    let y = b;
    let z = x + y;
    let kk = z + 1;
    let zzz = kk + 10;
    return a; // Returning the first element as a representative
}

sub start() {
    a = inbox();
    b = inbox();

    bubble_sort();
}

