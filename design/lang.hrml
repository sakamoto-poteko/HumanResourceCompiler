init floor_max = 2;

let glbvar = 12;

sub start()
{
    while (true) {
        let i1 = inbox();
        let i2 = inbox();
        let r;
        if (i1 > i2) {
            r = mul2(i1);
            outbox(r);
            floor[1] = r;
        } else {
            r = i2;
            outbox(r);
            floor[0] = r;
        }
        outbox(r);
    }
}

function mul2(num)
{
    return num + num;
}


